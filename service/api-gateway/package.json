{
  "name": "food-ordering-server",
  "version": "1.0.0",
  "main": "server.js",
"scripts": {
  "build": "docker-compose --env-file .env build",
  "up": "docker-compose --env-file .env up -d",
  "down": "docker-compose --env-file .env down",
  "logs": "docker-compose --env-file .env logs -f",
  "clean": "docker-compose --env-file .env down --volumes --remove-orphans",
  "restart": "npm run down && npm run build && npm run up",
  "health": "curl -s http://localhost:${GATEWAY_PORT:-5000}/health | jq .",
  "ps": "docker-compose --env-file .env ps",
  "shell": "docker-compose --env-file .env exec $(SERVICE) sh",
  "prune": "docker system prune -f && docker volume prune -f"
},
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "raw-body": "3.0.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.2.0",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.2.0",
    "food-ordering-server": "file:",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-firebase-storage": "^4.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "http-proxy-middleware": "3.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
